{
  "name": "jankaderabek",
  "version": "1.0.0",
  "description": "My personal website",
  "author": "jankaderabek",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "build-and-start": "npm run build && npm run start",
    "generate": "nuxt generate",
    "api": "json-server -w db.json -p 3001",
    "lint": "eslint --max-warnings 0 --ext .ts,.js,.vue --ignore-path .gitignore .",
    "deploy": "gh-pages -b master -d dist",
    "test": "cypress run",
    "wait-and-test": "wait-on http://localhost:3000 && npm run test",
    "ci-test": "run-p -r api build-and-start wait-and-test"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.2.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "nuxt": "^2.10.0",
    "vue-property-decorator": "latest",
    "vuex-class": "latest"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.3.0",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-config-typescript": "^0.1.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "cypress": "^3.4.1",
    "eslint": "^6.1.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "gh-pages": "^2.1.1",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "wait-on": "^3.3.0"
  }
}
